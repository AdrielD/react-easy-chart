    "d3-shape": "^0.2.2",
    "d3-time-format": "^0.4.0",
    "lodash.merge": "^4.0.1",
    "radium": "^0.17.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-faux-dom": "^2.5.0"
  }
